{
  "name": "talent-flow-webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --port 4201",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "CHROME_BIN=brave-browser ng test",
    "test:chrome": "CHROME_BIN=google-chrome ng test",
    "test:chromium": "CHROME_BIN=brave-browser ng test",
    "test:chrome-win": "set CHROME_BIN=C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe && ng test",
    "test:chrome-mac": "CHROME_BIN=/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome ng test",
    "test:rules": "firebase emulators:exec --only firestore 'cd tests && npm test'",
    "serve:ssr:talent-flow-webapp": "node dist/talent-flow-webapp/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.1",
    "@tailwindcss/postcss": "^4.1.10",
    "autoprefixer": "^10.4.21",
    "express": "^5.1.0",
    "firebase": "^10.9.0",
    "ngx-toastr": "^18.0.0",
    "postcss": "^8.5.5",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.10.7",
    "tailwindcss": "^4.1.10",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.2"
  }
}
